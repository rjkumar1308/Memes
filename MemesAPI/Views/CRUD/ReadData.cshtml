<html ng-app="appmodule">
<head>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="../Scripts/jquery-3.3.1.min.js"></script>
    <script src="../Scripts/bootstrap.min.js"></script>
    <script src="../Scripts/angular.min.js"></script>
    <script src="../Scripts/MyScripts/Module.js"></script>
    <script src="../Scripts/MyScripts/MemeService.js"></script>
    <script src="../Scripts/MyScripts/MemeController.js"></script>
</head>
<body ng-controller="memecontroller">
    <h1>Memes Information</h1>

    <h2>
        <span>{{userName}}</span>
        <input type="button" class="btn" value="LogOut" ng-click="logout()" />
    </h2>

    <table class="table table-striped table-striped table-bordered">
        <thead>
            <tr>
                <th>ImageId</th>
                <th>Url</th>
                <th>Tags</th>
            </tr>
        </thead>
        <tbody ng-repeat="meme2 in Memes">
            <tr ng-repeat="meme in meme2">
                <td>
                    <span>{{meme.ImageId}}</span>
                </td>
                <td>
                    <span><img src={{meme.Url}} height=30% width=20%></span>
                </td>
                <td>
                    <span>{{meme.Tags}}</span>
                </td>
            </tr>
        </tbody>
    </table>
    
    

</body>
</html>